{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "rallycoding/multi-client:latest",
      "hostname": "client",
      "memory": 128,
      "essential": false
    },
    {
      "name": "server",
      "image": "rallycoding/multi-server:latest",
      "hostname": "api",
      "memory": 128,
      "essential": false
    },
    {
      "name": "worker",
      "image": "rallycoding/multi-worker:latest",
      "memory": 128,
      "essential": false
    },
    {
      "name": "nginx",
      "image": "rallycoding/multi-nginx:latest",
      "memory": 128,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}